    "latest": "bun update --latest && bun check",
    "start": "bun src/cli.ts cli --cwd start-runtime",
    "check": "tsc --noEmit && eslint --cache --fix . && biome check --fix --unsafe . && knip",
    "dev:cli": "bun src/cli.ts cli --dev",
    "dev:add": "bun src/cli.ts add --dev",
