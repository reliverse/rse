  "description": "rse (prev. reliverse cli) all-in-one companion for bootstrapping and improving any kind of projects (especially web apps built with frameworks like Next.js) â€” whether you're kicking off something new or upgrading an existing app. It is also a little AI-powered toolbox in your terminal, ready to help with coding, refactoring, image gen, and more.",
  "scripts": {
    "pub": "dler pub",
    "dev": "bun dev:cli",
    "web:dev": "bun src/cli.ts web --dev",
    "web:start": "NODE_ENV=production bun src/app/web/index.tsx",
    "web:build": "bun build ./src/app/web/index.html --outdir=dist-web --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "gen:template": "bun dler pack src/templates --d template.ts --as-template",
    "libs:pack": "bun dler pack src/templates --output templates/better-t-stack",
    "libs:unpack": "bun dler unpack templates/better-t-stack --output src/templates/better-t-stack",
    "example": "bun cli-test.ts",
    "db": "drizzle-kit studio",
    "latest": "bun update --latest && bun check",
    "start": "bun src/cli.ts cli --cwd start-runtime",
    "check": "tsc --noEmit && eslint --cache --fix . && biome check --fix --unsafe . && knip",
    "dev:cli": "bun src/cli.ts cli --dev",
    "dev:add": "bun src/cli.ts add --dev",
    "dev:ai": "bun src/cli.ts ai --dev",
    "dev:clone": "bun src/cli.ts clone --dev",
    "dev:cmod": "bun src/cli.ts cmod --dev",
    "dev:env": "bun src/cli.ts env --dev",
    "dev:help": "bun src/cli.ts help --dev",
    "dev:init": "bun src/cli.ts init --dev",
    "dev:login": "bun src/cli.ts login --dev",
    "dev:logout": "bun src/cli.ts logout --dev",
    "dev:memory": "bun src/cli.ts memory --dev",
    "dev:mrse": "bun src/cli.ts mrse --dev",
    "dev:schema": "bun src/cli.ts schema --dev",
    "dev:studio": "bun src/cli.ts studio --dev",
    "dev:toolbox": "bun src/cli.ts toolbox --dev",
    "dev:update": "bun src/cli.ts update --dev",
    "dev:upload": "bun src/cli.ts upload --dev"
  },
