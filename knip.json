{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    ".config/**/*.ts",
    "eslint.config.js",
    "build.config.ts",
    "addons/**/*.ts",
    "src/mod.ts",
    "src/app/**/cmd.ts",
    "src/libs/**/*.ts",
    "src/templates/**/*.ts",
    "src/templates/**/*.tsx",
    "src/providers/better-t-stack/**/*.ts",
    "src/providers/better-t-stack/**/*.tsx"
  ],
  "project": ["eslint.config.js", "src/**/*.ts"],
  "ignoreExportsUsedInFile": true,
  "ignore": [
    "tests-runtime/",
    "dist-jsr/",
    "dist-npm/",
    "dist-libs/",
    "relinter-test.ts",
    "src/templates/**/*.ts",
    "src/templates/**/*.tsx",
    "src/providers/better-t-stack/**/*.ts",
    "src/providers/better-t-stack/**/*.tsx"
  ],
  "ignoreBinaries": [
    "code",
    "db:push",
    "brew",
    "choco",
    "atlas",
    "turso",
    "where",
    "which"
  ],
  "ignoreDependencies": [
    "octokit",
    "querystring",
    "@ai-sdk/svelte",
    "@ai-sdk/vue",
    "@tanstack/vue-query-devtools"
  ]
}
