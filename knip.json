{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "**/*.ts",
    "src/mod.ts",
    "src/app/**/cmd.ts",
    "src/libs/**/*.ts",
    "src/providers/better-t-stack/**/*.ts"
  ],
  "project": ["src/**/*.ts"],
  "ignoreExportsUsedInFile": true,
  "ignore": [
    "tests-runtime/",
    "dist-jsr/",
    "dist-npm/",
    "dist-libs/",
    "relinter-test.ts"
  ],
  "ignoreBinaries": [
    "code",
    "db:push",
    "brew",
    "choco",
    "atlas",
    "turso",
    "where",
    "which"
  ]
}
