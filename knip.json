{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    ".config/**/*.ts",
    "src/mod.ts",
    "src/app/**/cmd.ts",
    "src/libs/**/*.ts",
    "src/templates/**/*.ts",
    "src/templates/**/*.tsx"
  ],
  "project": ["src/**/*.ts"],
  "ignoreExportsUsedInFile": true,
  "ignore": [
    "tests-runtime/",
    "dist-jsr/",
    "dist-npm/",
    "dist-libs/",
    "relinter-test.ts",
    "src/templates/**/*.ts",
    "src/templates/**/*.tsx"
  ],
  "ignoreBinaries": [
    "build",
    "code",
    "db:push",
    "brew",
    "choco",
    "atlas",
    "turso",
    "where",
    "which"
  ],
  "ignoreDependencies": [
    "@webtui/css",
    "octokit",
    "querystring",
    "@ai-sdk/svelte",
    "@ai-sdk/vue",
    "@tanstack/svelte-form",
    "@tanstack/svelte-query",
    "@tanstack/vue-query-devtools"
  ]
}
