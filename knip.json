{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    ".config/**/*.ts",
    "src/mod.ts",
    "src/app/**/cmd.ts",
    "src/libs/**/*.ts",
    "src/providers/better-t-stack/**/*.ts"
  ],
  "project": ["src/**/*.ts"],
  "ignoreExportsUsedInFile": true,
  "ignore": [
    "tests-runtime/",
    "dist-jsr/",
    "dist-npm/",
    "dist-libs/",
    "relinter-test.ts"
  ],
  "ignoreBinaries": [
    "code",
    "db:push",
    "brew",
    "choco",
    "atlas",
    "turso",
    "where",
    "which"
  ],
  "ignoreDependencies": [
    "octokit",
    "querystring",
    "bun-plugin-tailwind",
    "@hookform/resolvers",
    "@radix-ui/react-label",
    "@radix-ui/react-select",
    "@radix-ui/react-slot",
    "class-variance-authority",
    "clsx",
    "lucide-react",
    "react-hook-form",
    "tailwind-merge",
    "tailwindcss-animate",
    "autoprefixer",
    "postcss"
  ]
}
